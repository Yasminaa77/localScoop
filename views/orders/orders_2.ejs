<body>

<header class="fixed_header">
    <!--Back button-->
    <div class="nav-wrapper seller-shop seller-landing-header">
        <div class="nav-wrapper--left seller-shop">
            <span>Sold Products</span>
        </div>
        <div class="nav-wrapper--right seller-shop">

            <a href="#"><span class="material-icons nav-icon seller-shop">mode_comment</span></a>
            <a href="#"><span class="material-icons nav-icon seller-shop">settings</span></a>
        </div>

    </div>
</header>
<div class="top_space"></div>

<main class="seller-orders2-body">
    <% for(let order of orderData) { %> 
        <div class="cardDiv">
            <div class="productClass">
                <div class="imgDiv">
                    <img src="<%= order.photo_file_path %>" alt="image of product"> 
                </div>
                <div class="textdiv">
                    <h6><%= order.product_name %></h6>
                    <h6><%= order.order_status %></h6>
                    <h6><%= order.how_many_days_ago %> </h6>
                </div>
            </div>
        </div>
    <% } %> 


